{
  "extends": ["@antfu", "prettier"],
  "rules": {
    "vue/component-tags-order": [
      "error",
      {
        "order": [["script", "template"], "style"]
      }
    ]
  },
  "overrides": [
    {
      "files": ["*.vue"],
      "rules": {
        // Multiple <script> tags.
        "import/first": "off"
      }
    }
  ]
}
